{
  "custom/kbd-backlight": {
    "format": "{icon} {percent}%",
    "format-icons": ["", "", "", "", "", "", "", "", ""],
    "return-type": "json",
    "exec": "~/.local/lib/hyde/kbd-backlight-status.sh --status",
    "on-scroll-up": "~/.local/lib/hyde/kbd-backlight-control.sh i",
    "on-scroll-down": "~/.local/lib/hyde/kbd-backlight-control.sh d",
    "min-length": 6,
    "tooltip": true,
    "tooltip-format": "Keyboard Backlight: {icon} {percent}%"
  }
}
